    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="somma, contatore, N" type="Integer" array="False" size=""/>
            <output expression="&quot;Somma dei numeri da 0 a N. Immetti N:&quot;" newline="True"/>
            <input variable="N"/>
            <assign variable="somma" expression="0"/>
            <assign variable="contatore" expression="1"/>
            <while expression="contatore &lt;= N">
                <assign variable="somma" expression="somma + contatore"/>
                <assign variable="contatore" expression="contatore + 1"/>
            </while>
            <output expression="&quot;La somma dei primi &quot; &amp; N &amp; &quot; numeri vale &quot; &amp; somma" newline="True"/>
        </body>
    </function>
</flowgorithm>
